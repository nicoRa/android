#include "<jni.h>"
#include "ar_uba_fi_pruebajni_NativeLib.h"

JNIEXPORT jstring JNICALL Java_ar_uba_fi_pruebajni_NativeLib_hello
(JNIEnv * env, jobject obj) {

	return (*env)->NewStringUTF(env, "Hola mundo");

}
JNIEXPORT jint JNICALL Java_ar_uba_fi_pruebajni_NativeLib_add
(JNIEnv * env, jobject obj, jint value1, jint value2) {

	return (value1 + value2);

}
